# name: test/sql/llm_batch.test
# description: Test LLM batch caching functions
# group: [llm]

require llm

# Test llm_hash function returns consistent hash
query I
SELECT llm_hash('test prompt', 'test-model') = llm_hash('test prompt', 'test-model');
----
true

# Test llm_hash with different prompts returns different hashes
query I
SELECT llm_hash('prompt 1', 'model') != llm_hash('prompt 2', 'model');
----
true

# Test llm_hash with different models returns different hashes
query I
SELECT llm_hash('same prompt', 'model-a') != llm_hash('same prompt', 'model-b');
----
true
